import random
print("WELCOME TO RANDOM PASSWORD GENERATOR...!!")
randomchar="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890!@#$%"
noofpwd=int(input("PLEASE ENTER THE NUMBER OF PASSWORDS TO BE GENERATE"))
pwdlen=int(input("PLEASE ENTER THE LENGTH OF THE PASSWORD"))
print("HERE ARE YOUR RANDOM PASSWORDS:")
for x in range(noofpwd):
 pwd=" "
 for chars in range(pwdlen):
       pwd=pwd+random.choice(randomchar)
 print(pwd)